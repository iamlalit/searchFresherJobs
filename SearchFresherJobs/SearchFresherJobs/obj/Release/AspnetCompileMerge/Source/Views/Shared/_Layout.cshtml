<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/fontawesome")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body ng-app="SearchFresherJobsApp" data-ng-controller="homeCtrl">
    @Html.Partial("_HeaderMenu")

    <div class="body-content hidden">
        @RenderBody()
    </div>
    <div class="pushOffFooter">

    </div>
    @Html.Partial("_footer")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/angularjs")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/Scripts/Custom")
    @RenderSection("scripts", required: false)</body>
</html>
